        <member name="M:PatientRegistrySystem.API.Controllers.UsersController.GetUser(System.Int32)">
            <summary>
            Get user by id number
            </summary>
            <param name="id">The id of the user you want to get</param>
            <returns>an IAction result</returns>
            <remarks> \
