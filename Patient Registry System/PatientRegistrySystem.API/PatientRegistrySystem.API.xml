            GET Users/id \
            </remarks>
            <response code="200">Return the requisted User</response>
        </member>
        <member name="M:PatientRegistrySystem.API.Controllers.UsersController.UpdateUser(PatientRegistrySystem.Domain.Dto.UserDto)">
            <summary>
            Update user by id number
            </summary>
            <param name="user">The user object you want to update the current user to</param>
            <returns>an IAction result</returns>
            <remarks>Sample requist() \
            PUT Users/id \
            { \
                "userId": 1, \
                "firstName": "Hussein", \
                "lastName": "Shukri", \
                "login": "35555", \
                "password": "h1234", \
                "email": "Hussein@Shukri.com", \
                "phone": "0595" \
            }</remarks>
            <response code="200">Return the updated User</response>
        </member>
        <member name="M:PatientRegistrySystem.API.Controllers.UsersController.DeleteUser(System.Int32)">
            <summary>
